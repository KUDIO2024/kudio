{
        "version": 2,
        "builds": [
                {
                        "src": "server.js",
                        "use": "@vercel/node"
                }
        ],
        "routes": [
                {
                        "src": "/public/(.*)",
                        "dest": "/public/$1"
                },
                {
                        "src": "/(.*)",
                        "dest": "/server.js"
                }
        ],
        "headers": [
                {
                        "src": "/(.*)",
                        "key": "Content-Security-Policy",
                        "value": "frame-ancestors 'self' https://kudio.co.uk"
                },
                {
                       "src": "/(.*)",
                        "key": "x-vercel-protection-bypass",
                        "value": "sncTiOAunzNTPkCtK5IyVWbK23qbL6t9"
                }
        ]
}
