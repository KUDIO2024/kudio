{
        "version": 2,
        "builds": [
                {
                        "src": "server.js",
                        "use": "@vercel/node"
                }
        ],
        "routes": [
                {
                        "src": "/public/(.*)",
                        "dest": "/public/$1"
                },
                {
                        "src": "/(.*)",
                        "dest": "/server.js"
                }
        ],
        "headers": [
                {
                        "source": "/(.*)",
                        "headers": [
                                {
                                        "key": "Content-Security-Policy",
                                        "value": "frame-ancestors 'self' https://kudio.co.uk"
                                },
                                {
                                        "key": "x-vercel-protection-bypass",
                                        "value": "sncTiOAunzNTPkCtK5IyVWbK23qbL6t9"
                                }
                        ]
                }
        ]
}
